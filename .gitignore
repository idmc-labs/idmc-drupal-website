# Files and directories suggested to be ignored by Drupal
# @see https://git.drupalcode.org/project/drupal/-/blob/9.2.x/example.gitignore

# Ignore directories generated by Composer
/vendor/
/core
/modules/contrib/
/themes/contrib/
/profiles/contrib/
/libraries/

# Ignore configuration files that may contain sensitive information.
sites/*/settings*.php
sites/*/services*.yml

# Ignore paths that contain user-generated content.
sites/*/files
sites/*/private

# Ignore SimpleTest multi-site environment.
sites/simpletest

# Ignore files generated by IDE
.idea/
.vscode/

# Ignore .env files as they are personal
/.env

# System
.DS_Store

# Project specific ignore paths
!sites/*/files/config
sites/all/libraries
sites/gidd
sites/displacement-downtown
sites/grid2016
sites/grid2017
sites/grid2018
sites/grid2019
sites/grid2020
sites/grid2021
sites/grid2022
sites/idi2021
sites/law-and-policy
*.csv
*.mp4

.htaccess
.htpasswd
